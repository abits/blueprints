<VirtualHost *:80>
  ServerName www.webgrind.vbox.local

  ServerAdmin admin@localhost

  ServerAlias webgrind.vbox.local
  DocumentRoot /srv/www/webgrind
  <Directory /srv/www/webgrind>
    Options Indexes FollowSymLinks MultiViews
    AllowOverride None
    Order allow,deny
    allow from all
  </Directory>
  ErrorLog /var/log/apache2/www.webgrind.vbox.local_error.log
  LogLevel warn
  CustomLog /var/log/apache2/www.webgrind.vbox.local_access.log combined
  ServerSignature Off
</VirtualHost>
